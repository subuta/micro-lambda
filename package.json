{
  "name": "play-with-lambda",
  "version": "1.0.0",
  "description": "docker-lambda testing",
  "main": "lib/index.js",
  "scripts": {
    "test": "run-p test:*",
    "test:lib": "jest",
    "test:it": "jest -c ./jest.config.integration.js",
    "build": "run-p build:*",
    "build:bundle": "rollup -c",
    "dev": "node -r esm node_modules/.bin/micro-dev example/handler.js",
    "start": "node -r esm node_modules/.bin/micro example/handler.js",
    "inspect": "node --inspect node_modules/.bin/micro-dev example/handler.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-lambda.git"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "mocha",
      "jest",
      "jasmine"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/subuta/play-with-lambda/issues"
  },
  "homepage": "https://github.com/subuta/play-with-lambda#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "docker-lambda": "^0.15.3",
    "esm": "^3.2.0",
    "jest": "^24.0.0",
    "micro": "^9.3.3",
    "micro-cors": "^0.1.1",
    "micro-dev": "^3.0.0",
    "microrouter": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.0-rc.1",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "serve-handler": "^5.0.8",
    "sinon": "^7.2.3",
    "standard": "^12.0.1",
    "supertest": "^3.4.2"
  },
  "dependencies": {
    "binary-case": "^1.1.4",
    "etag": "^1.8.1",
    "lodash": "^4.17.11",
    "type-is": "^1.6.16"
  }
}
